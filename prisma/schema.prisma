generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Size {
  TINY
  SMALL
  MEDIUM
  LARGE
  HUGE
  GARGANTUAN
}

model Race {
  id        String   @id @unique
  name      String
  main_race String
  size      Size
  speed     Float
  modifiers String[]
  traits    String[]
}
